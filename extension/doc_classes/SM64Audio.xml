<?xml version="1.0" encoding="UTF-8" ?>
<class name="SM64Audio" inherits="Object" deprecated="Use SM64AudioStreamPlayer instead." xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		Singleton for controlling the audio generated by [code]libsm64[/code].
	</brief_description>
	<description>
	</description>
	<tutorials>
	</tutorials>
	<methods>
		<method name="get_current_background_music">
			<return type="int" enum="SM64Audio.MusicID" />
			<description>
				Get the [enum MusicID] of the currently played music.
			</description>
		</method>
		<method name="play_music">
			<return type="void" />
			<param index="0" name="music_id" type="int" enum="SM64Audio.MusicID" />
			<param index="1" name="variant" type="bool" default="false" />
			<description>
				Start playing [param music_id]. If [param variant] is [code]true[/code], the functions plays the variant version (if aplicable).
			</description>
		</method>
		<method name="set_volume">
			<return type="void" />
			<param index="0" name="volume" type="float" />
			<description>
				Set the internal volume of the sounds played by [code]libsm64[/code].
			</description>
		</method>
		<method name="stop_background_music">
			<return type="void" />
			<param index="0" name="music_id" type="int" enum="SM64Audio.MusicID" />
			<description>
				Stops playing [param music_id].
			</description>
		</method>
		<method name="stop_current_background_music">
			<return type="void" />
			<description>
				Stops playing the current background music.
			</description>
		</method>
	</methods>
